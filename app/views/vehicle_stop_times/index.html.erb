<h1>Manage All Stop Time</h1>

<div class="container">
  <div class="row">
    <div class="col-lg-6 col-md-6 col-sm-6">
      <% if @vehicle_stop_times.any? %>
        <h3>Number of Trips: <%= @vehicle_stop_times.count %></h3>
        <ol class="times">
          <%= render @vehicle_stop_times %>
        </ol>
        <%= will_paginate @vehicle_stop_times %>
      <% end %>
    </div>
  </div>
</div>
