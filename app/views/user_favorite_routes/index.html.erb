<p id="notice"><%= notice %></p>

<h1>Listing User Favorite Routes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Vehicle route</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_favorite_routes.each do |user_favorite_route| %>
      <tr>
        <td><%= user_favorite_route.user %></td>
        <td><%= user_favorite_route.vehicle_route %></td>
        <td><%= link_to 'Show', user_favorite_route %></td>
        <td><%= link_to 'Edit', edit_user_favorite_route_path(user_favorite_route) %></td>
        <td><%= link_to 'Destroy', user_favorite_route, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Favorite route', new_user_favorite_route_path %>
