<h1>Manage All Users</h1>

<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <% if @users.any? %>
        <h3>Number of Users: <%= @users.count %></h3>
        <ol class="users">
          <%= render @users %>
        </ol>
      <% end %>
    </div>
  </div>
</div>