<p id="notice"><%= notice %></p>

<h1>Listing Vehicle Realtimes <%= link_to 'Update GeoLocations', vehicle_realtimes_pull_path %></h1>

<table>
  <thead>
    <tr>
      <th>Vehicle trip</th>
      <th>Vehicle route</th>
      <th>Vehicle stop</th>
      <th>Current status</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Updated At</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_realtimes.each do |vehicle_realtime| %>
      <tr>
        <td><%= vehicle_realtime.vehicle_trip.name %></td>
        <td><%= vehicle_realtime.vehicle_route.long_name %></td>
        <td><%= vehicle_realtime.vehicle_stop %></td>
        <td><%= vehicle_realtime.current_status %></td>
        <td><%= vehicle_realtime.lat %></td>
        <td><%= vehicle_realtime.long %></td>
        <td><%= vehicle_realtime.updated_at %></td>
        <td><%= link_to 'Show', vehicle_realtime %></td>
        <td><%= link_to 'Edit', edit_vehicle_realtime_path(vehicle_realtime) %></td>
        <td><%= link_to 'Destroy', vehicle_realtime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

